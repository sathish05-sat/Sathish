import java.util.Scanner;
class Main {
    public static void main(String[] args){
        Scanner data = new Scanner(System.in);
        System.out.println("Enter num");
        
        int num=data.nextInt();
        for( num=0;num<=10;num++){
            System.out.println(num);
        
            if(num==5)
            {
             
                break;
                
            }
            else{
               continue;
                
            }
        }
    }
}
