import java.util.Scanner;
class Main {
    public static void main(String[] args){
        Scanner data = new Scanner(System.in);
        System.out.println("Enter num");
        int num=data.nextInt();
        while(num>=0){
            num--;
            if((num%2)==0){
                System.out.println(num+"-Even");
            }
            else{
                System.out.println(num+"-Odd");
            }
        }
    }
}
