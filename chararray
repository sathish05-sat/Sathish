import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
    System.out.println("enter the name:");
    String x=sc.nextLine();
        
        char[]y=x.toCharArray();
       for(char a:y){
        System.out.println(a);
       }
    }
}
